{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feat"]
    },
    {
      "title": "## 🧪 Performance",
      "labels": ["perf"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## 🛠️ Development",
      "labels": ["refactor", "chore"]
    }
  ],
  "label_extractor": [
    {
      "pattern": "(feat|fix|perf|refactor|chore):",
      "target": "$1"
    }
  ],
  "tag_resolver": {
    "method": "semver",
    "filter": {
      "pattern": "v([0-9]+[.][0-9]+[.][0-9]+){1}-rc[0-9]+"
    }
  }
}