{
  "sort": "DESC",
  "categories": [
    {
      "title": "## 🚀 FEATURES",
      "labels": ["feat"]
    },
    {
      "title": "## 🧪 PERFORMANCE",
      "labels": ["perf"]
    },
    {
      "title": "## 🐛 FIXES",
      "labels": ["fix"]
    },
    {
      "title": "## 🛠️ TECH",
      "labels": ["refactor", "chore"]
    }
  ],
  "tag_resolver": {
    "method": "semver",
    "filter": {
      "pattern": "v([0-9]+[.][0-9]+[.][0-9]+){1}-rc[0-9]+"
    }
  },
  "pr_template": "#### **[PR#${{NUMBER}}](${{URL}}) | ${{TITLE}}**\n${{BODY}}\n",
  "template": "# 📎 CHANGELOG\n\n```\n${{FROM_TAG}} > ${{TO_TAG}}\n```\n\n${{CHANGELOG}}\n",
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000
}